{
    "name": "horde/whups",
    "description": "Ticket-tracking application",
    "type": "horde-application",
    "homepage": "https://www.horde.org/apps/whups",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "version": "4.0.0alpha1",
    "time": "2021-02-24",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "*",
        "php": "^5.3 || ^7",
        "horde/horde": "^5",
        "horde/auth": "^2",
        "horde/autoloader": "^2",
        "horde/compress": "^2",
        "horde/core": "^2.30",
        "horde/date": "^2",
        "horde/db": "^2",
        "horde/exception": "^2",
        "horde/form": "^2",
        "horde/group": "^2",
        "horde/mail": "^2",
        "horde/mime": "^2.10",
        "horde/mime_viewer": "^2",
        "horde/notification": "^2",
        "horde/perms": "^2",
        "horde/prefs": "^2",
        "horde/scheduler": "^2",
        "horde/share": "^2",
        "horde/support": "^2",
        "horde/text_filter": "^2",
        "horde/util": "^2",
        "horde/vfs": "^2",
        "horde/view": "^2",
        "ext-gettext": "*",
        "ext-json": "*"
    },
    "suggest": {},
    "autoload": {
        "psr-0": {
            "Whups": "lib/"
        },
        "psr-4": {
            "Horde\\Whups\\": "src/"
        }
    },
    "bin": [
        "bin/whups-bugzilla-import",
        "bin/whups-convert-datatree-shares-to-sql",
        "bin/whups-convert-sql-shares-to-sqlng",
        "bin/whups-convert-to-utf8",
        "bin/whups-git-hook",
        "bin/whups-mail-filter",
        "bin/whups-obliterate",
        "bin/whups-reminders",
        "bin/whups-svn-hook"
